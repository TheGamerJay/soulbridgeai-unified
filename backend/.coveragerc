[run]
source = .
omit = 
    */tests/*
    */test_*
    */venv/*
    */env/*
    */__pycache__/*
    */node_modules/*
    */htmlcov/*
    */soulbridgeai-frontend/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    pass

[html]
directory = htmlcov
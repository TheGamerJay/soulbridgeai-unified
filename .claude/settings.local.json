{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(find:*)",
      "Bash(for file in backend/modules/**/*.py)",
      "Bash(do python -m py_compile \"$file\")",
      "Bash(echo \"$file\")",
      "Bash(done)",
      "Bash(python3:*)",
      "Read(//c//**)",
      "Bash(for file in modules/**/*.py)",
      "Bash(break)",
      "Bash(echo \"Exit code: $?\")",
      "Bash(cat:*)",
      "Bash(curl:*)",
      "Bash(git ls-tree:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits"
  }
}
{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(python:*)",
      "Bash(timeout 10 sleep 10)",
      "Bash(timeout 8 sleep 8)",
      "Bash(taskkill:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(dir:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(curl:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits"
  }
}
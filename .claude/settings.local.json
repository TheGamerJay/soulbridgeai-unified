{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(npm run lint)",
      "Bash(npm run:*)",
      "Bash(python:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(sqlite3:*)",
      "Bash(node:*)",
      "Bash(curl:*)",
      "Bash(find:*)",
      "Bash(magick:*)",
      "Bash(git rm:*)",
      "Bash(git checkout:*)",
      "Bash(for:*)",
      "Bash(do sed -i 's/@rate_limit_moderate()/@rate_limit_moderate/g' \"$file\")",
      "Bash(done)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits"
  }
}